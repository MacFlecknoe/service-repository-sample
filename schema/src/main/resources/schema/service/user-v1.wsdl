	targetNamespace="urn:healthmedia:wsdl:user:v1"
	name="UserService">
	<documentation>Version 1.0</documentation>
	<types>
		<xsd:schema targetNamespace="urn:healthmedia:wsdl:user:v1">
			<xsd:element name="AddUser">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="in" type="xsd:string" />
