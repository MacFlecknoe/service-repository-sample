<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	A hack to make this classloader a top-level classloader. The actual value is ignored since DefaultDomain already exists.
	https://community.jboss.org/wiki/useJBossWebClassLoaderinJBoss5
-->
<classloading xmlns="urn:jboss:classloading:1.0" name="mywar.war"
	domain="DefaultDomain"
 
	parent-domain="Ignored"

	export-all="NON_EMPTY"
	import-all="true">
</classloading>